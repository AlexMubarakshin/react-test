{
  "name": "react-test",
  "version": "1.0.0",
  "description": "React test experiment with Typescript",
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "start": "node web/server.js",
    "native-start": "node node_modules/react-native/local-cli/cli.js start",
    "ios-bundle": "react-native bundle --dev false --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "ios-dev-bundle": "react-native bundle --entry-file index.ios.js --platform ios --bundle-output ios/main.jsbundle",
    "android-bundle": "react-native bundle --dev false --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "android-dev-bundle": "react-native bundle --entry-file index.android.js --platform android --bundle-output android/app/src/main/assets/index.android.bundle",
    "run-android": "react-native run-android",
    "web-bundle": "webpack --config web/webpack/webpack.config.js --progress --colors",
    "web-dev": "webpack-dev-server --content-base web/public/ --config web/webpack/webpack.config.js --port 3001 --inline --hot --colors"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harlockbcn/react-test.git"
  },
  "keywords": [
    "react"
  ],
  "author": "HarlockBcn",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/harlockbcn/react-test/issues"
  },
  "homepage": "https://github.com/harlockbcn/react-test#readme",
  "dependencies": {
    "classnames": "^2.2.5",
    "gulp": "^3.9.1",
    "native-base": "^0.5.12",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-native": "^0.32.1",
    "react-native-maps": "^0.11.0",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "redux-actions": "^0.12.0",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.4",
    "tslint": "^3.15.1"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.25.0",
    "html-webpack-plugin": "^2.22.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.1.1",
    "redux-logger": "^2.7.4",
    "style-loader": "^0.13.1",
    "tslint-react": "^1.1.0",
    "ts-loader": "^0.8.2",
    "typescript": "^2.0.6",
    "typings": "^1.5.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.1",
    "webpack-notifier": "^1.4.1"
  }
}
